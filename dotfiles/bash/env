export PATH=$PATH:$HOME/.rvm/bin
export GREP_COLOR="1;33"
export TERM=xterm-256color

# Source nvm script
. ~/Apps/nvm/nvm.sh
# Bash completion for nvm
[[ -r $NVM_DIR/bash_completion ]] && . $NVM_DIR/bash_completion
